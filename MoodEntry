package moodjournal;

import java.util.Date;

public class MoodEntry {
    private Date date;
    private int moodRating;
    private String notes;

    public MoodEntry(Date date, int moodRating, String notes) {
        this.date = date;
        this.moodRating = moodRating;
        this.notes = notes;
    }

    public Date getDate() {
        return date;
    }

    public int getMoodRating() {
        return moodRating;
    }

    public String getNotes() {
        return notes;
    }
}
